            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.idincidente, "idincidente", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("idincidente", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.idincidente, "", new { @class = "text-danger" })
      </div>


  </div>



  <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Crear" class="btn btn-primary" />
            </div>
        </div>
    </div>
}

<div>
